<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

        <bean   id="myCar"
                class="com.lesson.multyDependency.CarCompany"> <!-- new CarCompany() -->
                <property name="TheCar" ref="someCar"/> <!-- new CarCompany().setTheCar(new Germany(new Engine()))-->
        </bean>

        <bean   id="someCar"
                class="com.lesson.multyDependency.SomeCars.Germany">    <!-- new Germany(...) -->
                <constructor-arg ref="service" /> <!-- new Germany(new Engine()) -->
        </bean>

        <bean id="service"
                class="com.lesson.multyDependency.CarService.Engine"> <!-- new Engine() -->

        </bean>
</beans>